# =============================================================================
# WEB AUTOMATION & SECURITY
# =============================================================================

# Web Automation Settings
SELENIUM_DRIVER_PATH=/usr/local/bin/chromedriver
SELENIUM_HEADLESS=true
SELENIUM_TIMEOUT=30

# Security Settings
SESSION_TIMEOUT=3600
MAX_LOGIN_ATTEMPTS=5
ENCRYPTION_ALGORITHM=AES-256

# =============================================================================
# PLATFORM DEPLOYMENT
# =============================================================================

# Port Configuration
PORT=5000
HOST=0.0.0.0

# Redis Configuration (for production scaling)
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=your-redis-password

# Email Configuration (for notifications)
SMTP_SERVER=smtp.mail.com
SMTP_PORT=587
SMTP_USERNAME=your-email@gmail.com
SMTP_PASSWORD=your-email-app-password

# =============================================================================
# MONITORING & LOGGING
# =============================================================================

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE_PATH=logs/agentic.log
LOG_MAX_SIZE=10MB
LOG_BACKUP_COUNT=5

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090

# =============================================================================
# ADVANCED FEATURES
# =============================================================================

# AI Model Configuration
DEFAULT_AI_MODEL=gpt-3.5-turbo
MAX_TOKENS=2048
TEMPERATURE=0.7

# Agent System
MAX_CONCURRENT_AGENTS=10
AGENT_TIMEOUT=300
ENABLE_DYNAMIC_AGENTS=true

# External Knowledge
ENABLE_EXTERNAL_KNOWLEDGE=true
KNOWLEDGE_CACHE_TTL=3600
WIKIPEDIA_API_ENABLED=true

# =============================================================================
# DEPLOYMENT SPECIFIC
# =============================================================================

# Railway Deployment
RAILWAY_ENVIRONMENT=production

# Vercel Deployment  
VERCEL_URL=your-app.vercel.app

# Netlify Deployment
NETLIFY_SITE_ID=your-netlify-site-id

# Firebase Deployment
FIREBASE_PROJECT_ID=your-firebase-project-id

# Render Deployment
RENDER_SERVICE_ID=your-render-service-id

